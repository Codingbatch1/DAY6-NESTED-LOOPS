/*
5  4  3  2  1
2  2  2  2  2
5  4  3  2  1
4  4  4  4  4
5  4  3  2  1 */
import java.util.*;
class p2
{
public static void main(String args[])
{
int n=5;
int i,j;
for(i=1;i<=n;i++)
  {
      if(i%2 == 0)
      {
          for(j=1;j<=n;j++)
             System.out.print(i+"  ");
       }
    else
        {
             for(j=n;j>=1;j--)
             System.out.print(j+"  ");
          }                 

     System.out.print("\n");
}
}
}


