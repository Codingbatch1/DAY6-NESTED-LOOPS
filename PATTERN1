/* 1  0  1
   0  1  0
   1  0  1*/
   class pt2
{
public static void main(String args[])
{
int i,j,temp=1;
for(i=1;i<=5;i++)
{
for(j=1;j<=5;j++)
  {
    System.out.print(temp+"  "); // 1
    if(temp==0)
       temp=1;  
    else
        temp=0;
   }
    
   System.out.println();
}
}
}

     
        
