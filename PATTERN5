/*
1  1  1  1  1
1  2  3  4  5
3  3  3  3  3
1  2  3  4  5
5  5  5  5  5 */
import java.util.*;
class p2
{
public static void main(String args[])
{
int n=5;
int i,j;
for(i=1;i<=n;i++)
  {
      if(i%2 == 0)
      {
          for(j=1;j<=n;j++)
             System.out.print(j+"  ");
       }
    else
        {
             for(j=n;j>=1;j--)
             System.out.print(i+"  ");
          }                 

     System.out.print("\n");
}
}
}
